<% content_for :page_title do %>
  Products import from CSV
<% end %>

<%= form_tag({controller: "spree/admin/products_imports", action: "create"}, multipart: true) do %>
  <div>
    <%= label_tag "CSV file to import" %>
    <%= file_field_tag :csv_file %>
  </div>
  <div class="action_buttons">
    <%= submit_tag 'Submit' %>
    <%= link_to 'Cancel', admin_products_path %>
  </div>
<% end %>
